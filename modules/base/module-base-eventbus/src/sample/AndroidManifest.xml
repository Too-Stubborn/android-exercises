<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xuhj.android.base.eventbus">

    <application
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">

        <activity android:name="com.xuhj.android.base.eventbus.sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.xuhj.android.base.eventbus.sample.eventbus.EventBusReceiverActivity" />
        <activity android:name="com.xuhj.android.base.eventbus.sample.eventbus.EventBusSendActivity" />
        <activity android:name="com.xuhj.android.base.eventbus.sample.rxbus.RxBusReceiverActivity" />
        <activity android:name="com.xuhj.android.base.eventbus.sample.rxbus.RxBusSendActivity" />

    </application>

</manifest>